{
  "plugins": {
    "start": "node index.js"
  },
  "env": {
    "SESSION_ID": {
      "required": true,
      "description": "Your WhatsApp session ID (Baileys MD format)"
    },
    "ALIVE_IMG": {
      "required": false,
      "default": "https://github.com/DarkChenu/Dark-Chenu/blob/main/5faa8a2e-62c8-49bc-bf78-adaff8cc2c2d.png?raw=true",
      "description": "Alive command image URL"
    },
    "ALIVE_MSG": {
      "required": false,
      "default": "*Helloüëã DARKZ-MD Is Alive Nowüòç*",
      "description": "Custom alive message"
    },
    "MOVIE_API_KEY": {
      "required": false,
      "default": "sky|51c173810f25c71beb51b431867187b736b89c28",
      "description": "API key for movie plugin (Sinhalasub)"
    },
    "OPENAI_API_KEY": {
      "required": false,
      "description": "Your OpenAI API key (if using ChatGPT features)"
    },
    "GEMINI_API_KEY": {
      "required": false,
      "description": "Google Gemini API key"
    },
    "BOT_OWNER": {
      "required": true,
      "description": "Owner's number (e.g., 94762877975)"
    },
    "OWNER_NUMBER": {
      "required": true,
      "description": "Comma-separated list of owner numbers"
    },
    "AUTO_READ_STATUS": {
      "required": false,
      "default": "true",
      "description": "Enable or disable auto status reading (true/false)"
    }
  }
}
